{
  "compilerOptions": {
    "baseUrl": "./src/components.ts/*",
    "outDir": "./src/components/",
    "strictNullChecks": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "jsx": "preserve",
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "noImplicitAny": false,
    "target": "es6",
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}
